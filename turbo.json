{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "worker": {
      "persistent": true,
      "cache": false
    }
  }
}