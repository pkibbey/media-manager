    "file-type": "^21.0.0",
    "geist": "1.4.2",
    "lucide-react": "0.511.0",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "nsfwjs": "^4.2.1",
    "ollama": "^0.5.15",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "3.3.0",
    "tw-animate-css": "^1.3.0",
    "uuid": "^11.1.0",
    "xxhashjs": "^0.2.2",
    "zod": "3.25.30",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
