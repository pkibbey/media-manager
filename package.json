{
  "name": "media-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TF_METAL_DEVICE=1 NODE_OPTIONS='--max-old-space-size=8192' next dev",
    "build": "next build",
    "start": "TF_METAL_DEVICE=1 NODE_OPTIONS='--max-old-space-size=8192' next start",
    "server": "node duplicate-detection-api.js",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "types": "supabase gen types typescript --local > src/types/supabase.ts",
    "sync": "supabase migrations up --local --include-all && pnpm types && pnpm format",
    "knip": "knip",
    "knip:fix": "knip --fix --allow-remove-files",
    "worker:object": "tsx src/workers/object-detection-worker.ts",
    "worker:advanced": "tsx src/workers/advanced-analysis-worker.ts",
    "worker:content": "tsx src/workers/content-warnings-worker.ts",
    "worker:duplicates": "tsx src/workers/duplicates-worker.ts",
    "worker:exif": "tsx src/workers/exif-worker.ts",
    "worker:thumbnail": "tsx src/workers/thumbnail-worker.ts",
    "workers": "tsx src/workers/start-workers.ts"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/supabase-js": "2.49.8",
    "@tailwindcss/postcss": "^4.1.8",
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "bullmq": "^5.53.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "exiftool-vendored": "^30.2.0",
    "express": "^5.1.0",
    "file-type": "^21.0.0",
    "geist": "1.4.2",
    "ioredis": "^5.6.1",
    "lucide-react": "0.511.0",
    "next": "^15.3.3",
    "next-themes": "^0.4.6",
    "nsfwjs": "^4.2.1",
    "ollama": "^0.5.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.2",
    "sonner": "^2.0.4",
    "tailwind-merge": "3.3.0",
    "tw-animate-css": "^1.3.2",
    "uuid": "^11.1.0",
    "xxh3-ts": "^2.0.1",
    "zod": "^3.25.42",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "19.1.5",
    "autoprefixer": "^10.4.21",
    "knip": "^5.59.1",
    "postcss": "^8.5.4",
    "supabase": "^2.23.4",
    "tailwindcss": "^4.1.8",
    "tsx": "^4.19.4"
  }
}