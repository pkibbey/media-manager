{
  "name": "media-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TF_METAL_DEVICE=1 NODE_OPTIONS='--max-old-space-size=8192' next dev",
    "build": "next build",
    "start": "TF_METAL_DEVICE=1 NODE_OPTIONS='--max-old-space-size=8192' next start",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "types": "supabase gen types typescript --local > src/types/supabase.ts",
    "sync": "supabase migrations up --local --include-all && pnpm types && pnpm format",
    "knip": "knip",
    "knip:fix": "knip --fix --allow-remove-files"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/supabase-js": "2.49.8",
    "@tailwindcss/postcss": "^4.1.7",
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
