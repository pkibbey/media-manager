{
  "name": "media-manager",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@11.4.1",
  "workspaces": [
    "packages/workers/*",
    "packages/web-ui",
    "packages/shared"
  ],
  "scripts": {
    "dev": "turbo run dev --concurrency 12",
    "web": "turbo run web --concurrency 12",
    "build": "turbo run build",
    "workers": "turbo run worker",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "knip": "knip",
    "types": "npx supabase gen types typescript --local > packages/shared/types/supabase-types.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.15.30",
    "knip": "^5.60.2",
    "supabase": "^2.24.3",
    "turbo": "^2.5.4"
  }
}